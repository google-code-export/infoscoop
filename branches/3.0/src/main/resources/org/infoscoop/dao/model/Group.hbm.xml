<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Group"
		table="is_groups"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.String"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="true"
			length="255"
		/>


		<set name="Users" table="is_user_group">
			<key column="fk_group_id"/>
			<many-to-many class="User" column="fk_user_id"/>
		</set>


	</class>
</hibernate-mapping>