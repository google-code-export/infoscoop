<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Widget"
		table="IS_WIDGETS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<generator class="native">
				<param name="sequence">is_widgets_id_seq</param>
			</generator>
		</id>
		
		<property
			name="Tabid"
			column="TABID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="Deletedate"
			column="DELETEDATE"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="Createdate"
			column="CREATEDATE"
			type="java.lang.Long"
			not-null="true"
		/>
		<property
			name="Widgetid"
			column="WIDGETID"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="FkDomainId"
			column="fk_domain_id"
			type="integer"
		/>
		<property
			name="Uid"
			column="`UID`"
			type="string"
			not-null="false"
			length="150"
		/>

		<property
			name="Column"
			column="`COLUMN`"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Siblingid"
			column="SIBLINGID"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="Parentid"
			column="PARENTID"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="Href"
			column="HREF"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="Title"
			column="TITLE"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="Type"
			column="TYPE"
			type="string"
			not-null="false"
			length="1024"
		/>
		
		<property
			name="Isstatic"
			column="ISSTATIC"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Ignoreheader"
			column="IGNOREHEADER"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Noborder"
			column="NOBORDER"
			type="integer"
			not-null="false"
			length="10"
		/>
		
		<many-to-one
			name="MenuItem"
			column="fk_menu_id"
			class="MenuItem"
		>
		</many-to-one>

	</class>
</hibernate-mapping>