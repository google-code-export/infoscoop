<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="OAuth3LeggedConsumer"
		table="IS_OAUTH_3Legged_CONSUMERS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native">
				<param name="sequence">is_oauth_consumers_id_seq</param>
			</generator>
		</id>

		<property
			name="GadgetUrl"
			column="gadget_url"
			type="string"
			not-null="true"
			length="1024"
		/>
		<property
			name="GadgetUrlKey"
			column="gadget_url_key"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="ServiceName"
			column="service_name"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="ConsumerKey"
			column="consumer_key"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ConsumerSecret"
			column="consumer_secret"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="SignatureMethod"
			column="signature_method"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="IsUpload"
			column="is_upload"
			type="integer"
			not-null="true"
		/>

	</class>	
</hibernate-mapping>