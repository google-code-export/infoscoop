<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="MenuPosition"
		table="IS_MENU_POSITIONS"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="MENUPOSITIONPK">
			<key-property
				name="Id"
				type="string"
				column="position"
			/>
			<key-property
				name="FkDomainId"
				column="fk_domain_id"
				type="integer"
			/>
		</composite-id>
		
		<many-to-one
			name="FkMenuTree"
			column="fk_menu_tree_id"
			class="MenuTree"
			not-null="true"
			outer-join="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>