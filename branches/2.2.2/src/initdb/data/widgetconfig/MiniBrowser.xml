<?xml version="1.0" encoding="UTF-8"?>
<widgetConfiguration autoRefresh="false" displayFlag="false" type="MiniBrowser" title="!{lb_MiniBrowser}" singleton="true" icon="minibrowser.gif"> 
	<WidgetPref datatype="bool" display_name="!{lb_titleEdit}" name="titleEdit" value="true"/>
	<UserPref default_value="true" display_name="!{lb_openWidget}" datatype="hidden" name="openWidget"/>
	<UserPref default_value="" display_name="!{lb_webSiteURL}" datatype="hidden" name="url" admin_datatype="url"/>
	<UserPref default_value="auto" display_name="!{lb_headerLinkDisplay}" datatype="enum" name="itemDisplay">
		<EnumValue display_value="!{lb_portalframe}" value="inline"/>
		<EnumValue display_value="!{lb_newWindow}" value="newwindow"/>
		<EnumValue display_value="!{lb_targetAutoSelect}" value="auto"/>
	</UserPref>
	<UserPref default_value="140" display_name="!{lb_heightPixel}" datatype="string" name="height"/>
	<Header/>
	<Content className="MiniBrowser" type="javascript"/>
</widgetConfiguration>
