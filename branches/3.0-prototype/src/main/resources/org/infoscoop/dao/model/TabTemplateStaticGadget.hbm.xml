<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="TabTemplateStaticGadget"
		table="IS_TAB_TEMPLATE_STATIC_GADGETS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="ContainerId"
			column="container_id"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="IgnoreHeader"
			column="IGNOREHEADER"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="NoBorder"
			column="NOBORDER"
			type="integer"
			not-null="false"
			length="10"
		/>
		<many-to-one cascade="save-update"
			name="GadgetInstance"
			column="fk_gadget_instance_id"
			class="GadgetInstance"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="FkTabTemplate"
			column="fk_tabtemplate_id"
			class="TabTemplate"
			not-null="false"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>