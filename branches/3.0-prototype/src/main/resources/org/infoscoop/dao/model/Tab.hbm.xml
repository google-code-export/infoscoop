<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="Tab"
		table="IS_TABS"
	>
		<composite-id name="Id" class="TABPK">
			<key-property name="FkDomainId" column="fk_domain_id" type="integer"/>
			<key-property name="Uid" column="`UID`" type="string"/>
			<key-property name="Id"	column="ID"	type="string"/>
		</composite-id>
		<property
			name="Name"
			column="NAME"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="Order"
			column="`ORDER`"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Type"
			column="TYPE"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Data"
			column="DATA"
			type="text"
			not-null="false"
		/>
		<property
			name="TemplateTimestamp"
			column="template_timestamp"
			type="timestamp"
		/>
	</class>	
</hibernate-mapping>