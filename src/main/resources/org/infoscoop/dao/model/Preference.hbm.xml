<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class name="Preference" table="IS_PREFERENCES">
		<composite-id name="Id" class="PreferencePK">
			<key-property
				name="FkDomainOd"
				column="fk_domain_id"
				type="integer"
			/>
			<key-property
				name="Uid"
				column="UID"
				type="string"
			/>
		</composite-id>

		<property
			name="Data"
			column="DATA"
			type="text"
			not-null="true"
		/>

	</class>
</hibernate-mapping>