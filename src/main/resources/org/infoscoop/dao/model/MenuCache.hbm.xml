<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="MenuCache"
		table="IS_MENUCACHES"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="MENUCACHEPK">
			<key-property
				name="FkDomainId"
				column="fk_domain_id"
				type="integer"
			/>
			<key-property
				name="UrlKey"
				column="URL_KEY"
				type="string"
			/>
			<key-property
				name="Uid"
				column="`UID`"
				type="string"
			/>
		</composite-id>

		<property
			name="MenuIds"
			column="MENUIDS"
			type="binary"
			not-null="false"
		/>


	</class>	
</hibernate-mapping>