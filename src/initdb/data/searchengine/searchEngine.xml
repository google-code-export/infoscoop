<?xml version="1.0" encoding="UTF-8"?>
<searchEngines>
	<defaultSearch>
		<searchEngine id="google" title="Google" retrieveUrl="http://www.google.com/search?hl=en&amp;q=">
			<countRule method="regexp" value="(?:About)? ([0-9,]+) results|of about &lt;b&gt;([0-9,]+)&lt;/b&gt; for"/>
		</searchEngine>
		<searchEngine id="bing" title="Bing" retrieveUrl="http://www.bing.com/search?mkt=en-US&amp;q=">
			<countRule method="regexp" value="of ([0-9,]+) results"/>
		</searchEngine>
		<searchEngine id="yahoo" title="Yahoo" retrieveUrl="http://search.yahoo.com/search?ei=UTF-8&amp;p=">
			<countRule method="regexp" value="([0-9,]+)\s*&lt;/strong&gt;\s*results for"/>
		</searchEngine>
	</defaultSearch>
	<rssSearch>
		<searchEngine id="googlesite" retrieveUrl="http://www.google.com/search?hl=en&amp;q=site%3A$1%20">
			<rssPattern>^http://([^/]+)/.*</rssPattern>
			<countRule method="regexp" value="(?:About)? ([0-9,]+) results|of about &lt;b&gt;([0-9,]+)&lt;/b&gt; for"/>
		</searchEngine>
	</rssSearch>
</searchEngines>
