<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="OAuthConsumerProp"
		table="IS_OAUTH_CONSUMERS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
		</id>
		<property
			name="ServiceName"
			column="service_name"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="ConsumerKey"
			column="consumer_key"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ConsumerSecret"
			column="consumer_secret"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="SignatureMethod"
			column="signature_method"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Description"
			column="description"
			type="string"
		/>		
		<property
			name="IsUpload"
			column="is_upload"
			type="integer"
			not-null="true"
		/>
		<set name="OAuthGadgetUrl" lazy="true" inverse="true">  
            <key>  
                <column name="fk_oauth_id" not-null="true" />  
            </key>  
            <one-to-many class="OAuthGadgetUrl" />  
        </set>
        <set name="OAuthToken" lazy="true" inverse="true">  
            <key>  
                <column name="fk_oauth_id" not-null="true" />  
            </key>
            <one-to-many class="OAuthToken" />  
        </set>
	</class>	
</hibernate-mapping>