<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.infoscoop.dao.model">
	<class
		name="OAuthGadgetUrl"
		table="IS_OAUTH_GADGET_URLS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native">
				<param name="sequence">is_oauth_gadget_url_id_seq</param>
			</generator>
		</id>

		<property
			name="FkOauthId"
			column="fk_oauth_id"
			type="string"
			not-null="true"
			length="64"
		/>		
		<property
			name="GadgetUrl"
			column="gadget_url"
			type="string"
			not-null="true"
			length="1024"
		/>
		<property
			name="GadgetUrlKey"
			column="gadget_url_key"
			type="string"
			not-null="true"
			length="255"
		/>

	</class>	
</hibernate-mapping>